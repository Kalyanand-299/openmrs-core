pipeline {
    agent {label 'python'} 
    stages {
        stage('clone') { 
            steps {
                git url:'git@github.com:Kalyanand-299/openmrs-core.git' , branch : 'master' 
            }
        }
        stage('install') { 
            steps {
                sh 'mvn clean install' 
            }
        }
    }
}